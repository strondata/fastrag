        "max_value": 10000
      },
      "meta": {
        "notes": "Ensure we have at least one employee and not more than 10k"
      }
    },
    {
      "expectation_type": "expect_column_to_exist",
      "kwargs": {
        "column": "employee_id"
      }
    },
    {
      "expectation_type": "expect_column_to_exist",
      "kwargs": {
        "column": "name"
      }
    },
    {
      "expectation_type": "expect_column_to_exist",
      "kwargs": {
        "column": "age"
      }
    },
    {
      "expectation_type": "expect_column_to_exist",
      "kwargs": {
        "column": "salary"
      }
    },
    {
      "expectation_type": "expect_column_to_exist",
      "kwargs": {
        "column": "department"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_unique",
      "kwargs": {
        "column": "employee_id"
      },
      "meta": {
        "notes": "Employee IDs must be unique"
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "employee_id"
      }
    },
    {
      "expectation_type": "expect_column_values_to_not_be_null",
      "kwargs": {
        "column": "name"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_of_type",
      "kwargs": {
        "column": "employee_id",
        "type_": "int64"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": {
        "column": "age",
        "min_value": 18,
        "max_value": 100
      },
      "meta": {
        "notes": "Age must be between 18 and 100"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_between",
      "kwargs": {
        "column": "salary",
        "min_value": 0,
        "max_value": 1000000
      },
      "meta": {
        "notes": "Salary must be positive and reasonable"
      }
    },
    {
      "expectation_type": "expect_column_values_to_be_in_set",
      "kwargs": {
        "column": "department",
        "value_set": [
          "Engineering",
          "Sales",
          "HR",
          "Marketing",
          "Finance",
          "Operations"
        ]
      },
      "meta": {
        "notes": "Department must be one of the allowed values"
      }
    },
    {
      "expectation_type": "expect_column_mean_to_be_between",
      "kwargs": {
        "column": "salary",
        "min_value": 40000,
        "max_value": 150000
      },
      "meta": {
        "notes": "Average salary should be within reasonable range"
      }
    }
  ],
  "data_asset_type": "Dataset",
  "meta": {
